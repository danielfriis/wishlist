<li class="white-box create-list">
<%= form_for([current_user, @list]) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="input-append">
    <%= f.text_field :name, placeholder: "Create new list...", id:"appendedInputButton" %>
    <%= f.submit "+", class: "btn" %>
  </div>
<% end %>
</li>