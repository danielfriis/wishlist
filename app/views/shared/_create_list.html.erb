<div class="white-box">
<%= form_for(@list) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="input-append">
    <%= f.text_field :name, placeholder: "Create new list...", id:"appendedInputButton", class: "span4" %>
    <%= f.submit "Create list", class: "btn" %>
  </div>
<% end %>
</div>