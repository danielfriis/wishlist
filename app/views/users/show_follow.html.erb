<% provide(:title, @title) %>
<div class="row">
  <div class="span12 user_info">
    <div class="row">
      <%= render partial: 'shared/user_info', locals: {:user => @user} %>
    </div>
  </div>
</div>
<div class="row">
  <div class="span9">
    <h1><%= @title %></h1>
    <% if @users.any? %>
      <ul class="users">
        <%= render @users %>
      </ul>
      <%= will_paginate @users %>
    <% end %>
  </div>
  <aside class="pull-right span3">
    <%= render 'shared/aside_lists' %>
  </aside>
</div>