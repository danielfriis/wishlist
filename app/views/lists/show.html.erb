<% provide(:title, @list.name) %>
<div class="row">
  <div class="span12 user_info">
    <%= render 'shared/user_info' %>
  </div>

  <div class="span8">
    <% if @list.items.any? %>
      <ol class="wishes white-box">
        <%= render @wishes %>
      </ol>
    <% else %>
      <%= render 'shared/list_wo_wish' %>
    <% end %>
    <%= render 'shared/list_add_wish' %>  
  </div>

  <aside class="pull-right span4">
    <%= render 'shared/aside_share' %>
    <%= render 'shared/aside_lists' %>
  </aside>

</div>