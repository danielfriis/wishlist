<% if current_page?(user_path(@user)) %> 
<!-- Profile lists -->
	<div class="span6">
		<div class="white-box user_list">
			
				<% if current_user?(list.user) %>
				  <%= link_to "&times;".html_safe, [current_user, list], class: "close", method: :delete,
				                                data: { confirm: "You sure?" },
				                                title: list.name %>
				<% end %>
			
			<%= link_to user_list_path(@user, list) do %>
				<div class="user_list_title">
					<h3><%= list.name %></h3>
					<p class="muted" id="wishes_count"><%= pluralize(list.wishes.count, "wish") %></p>
				</div>
			<% end %>

			<ul class="user_list_wishes <% if current_user?(list.user) %>dragndrop<% end %> unstyled" id="wishes_<%= list.id %>" data-update-url="<%= sort_wishes_url %>" data-list-id="<%= list.id %>">

				<!-- List wishes -->
				<% if list.wishes.any? %>
					<% list.wishes.includes(:item).rank(:row_order).each do |wish| %>
						<li class="user_list_wish" id="wish_<%= wish.id %>" data-item-id="<%= wish.id %>">
								<div class="block pull-left">
									<%= link_to wish.item do %>	
										<%= image_tag wish.item.image_url(:thumb).to_s %>
									<% end %>
								</div>

								<div class="block">
									<span class="content">
										<%= link_to wish.item do %>	
											<h4><%= wish.item.title %></h4>
										<% end %>
										<span class="muted">
									    <p><%= wish.note %></p>
								 		</span>
										<span class="timestamp">
									    Added <%= time_ago_in_words(wish.created_at) %> ago.
								 		</span>
									</span>
								</div>

							

						</li>
					<% end %>
				<% end %>
				<%= render partial: 'shared/list_wo_wish', locals: {list: list} %>
			</ul>
			<% if current_user?(list.user) %>
				<%= link_to "Add wish", new_item_path, {remote: true, 'data-toggle' => 'modal', 'data-target' => "#make_wish", id: "add_wish", class: "user_list_add_wish"} %>
			<% end %>
		</div>
	</div>
<% else %>
<!-- Aside sidebar lists -->
	<li class="aside_user_list_wish">
		<div class="block pull-left">
			<%= image_tag(list.wishes.last.item.image_url(:thumb).to_s, class: "img-rounded") if list.wishes.last %>
		</div>
	  <div class="block">
	  	<span class="content">
	  		<span><%= link_to list.name, user_list_path(@user, list) %></span>
			  <span class="timestamp">
			    Updated <%= time_ago_in_words(list.updated_at) %> ago.			        
			  </span>
	  	</span>
	  </div>
	</li>
<% end %>